                  <div class="border-bottom color-border-secondary py-3">
                    <!-- '"` --><!-- </textarea></xmp> -->
                    <form
                      class="d-block d-md-flex"
                      data-autosearch-results-container="user-repositories-list"
                      aria-label="Repositories"
                      role="search"
                      action="/<%= login %>"
                      accept-charset="UTF-8"
                      method="get"
                    >
                      <div class="mb-3 mb-md-0 mr-md-3 flex-auto">
                        <input type="hidden" name="tab" value="repositories" />
                        <input
                          type="search"
                          id="your-repos-filter"
                          name="q"
                          class="form-control width-full"
                          placeholder="Find a repository…"
                          autocomplete="off"
                          aria-label="Find a repository…"
                          value=""
                          data-throttled-autosubmit=""
                        />
                      </div>

                      <div class="d-flex">
                        <details
                          class="details-reset details-overlay position-relative mr-2"
                          id="type-options"
