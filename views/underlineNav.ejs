              data-hydro-click-hmac="01fd4b0c44cd3712047e59685cf0a0480c4889cbb278fef654f9a4c1d7bdeaf4"
              href="/<%= login %>"
            >
              <svg
                class="octicon octicon-book UnderlineNav-octicon hide-sm"
                height="16"
                viewBox="0 0 16 16"
                version="1.1"
                width="16"
                aria-hidden="true"
